---
duffy-new:
  topology:
    topology_name: duffy-new
    resource_groups:
      - resource_group_name: duffy-new
        resource_group_type: duffy
        resource_definitions:
          - role: duffy_node
            name: duffy
            version: "{{ duffy_new_version | default('7') }}"
            arch: "{{ duffy_new_arch | default('x86_64') }}"
            count: 1
        credentials: duffy.key
  layout:
    inventory_layout:
      vars:
        hostname: __IP__
      hosts:
        example-node:
          count: 1
          host_groups:
            - example
